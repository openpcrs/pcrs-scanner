{
  "name": "pcrs-scanner",
  "version": "0.0.0",
  "repository": "https://github.com/openpcrs/pcrs-scanner.git",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "xo"
  },
  "dependencies": {
    "basic-ftp": "^5.0.3",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "got": "^13.0.0"
  },
  "devDependencies": {
    "xo": "^0.56.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off",
      "import/no-unassigned-import": "off",
      "import/order": "off"
    }
  },
  "engines": {
    "node": ">= 18.12 < 19"
  }
}
