{
  "name": "pcrs-scanner",
  "version": "0.0.0",
  "repository": "https://github.com/openpcrs/pcrs-scanner.git",
  "private": true,
  "type": "module",
  "dependencies": {},
  "devDependencies": {
    "xo": "^0.56.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off",
      "import/no-unassigned-import": "off",
      "import/order": "off"
    }
  },
  "engines": {
    "node": ">= 18.12 < 19"
  }
}
